{
  "compilerOptions": {
    "target": "esnext",
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "types": ["./node_modules/@internal/typescript"],
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@internal/typescript/node_modules/@types"
    ]
  }
}
