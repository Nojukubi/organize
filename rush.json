{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.70.0",
  "pnpmVersion": "6.32.13",
  "pnpmOptions": {
    "useWorkspaces": true
  },
  "projectFolderMaxDepth": 5,
  "suppressNodeLtsWarning": true,
  "ensureConsistentVersions": true,
  "projects": [
    {
      "packageName": "@organize/website",
      "projectFolder": "apps/website"
    },
    {
      "packageName": "@organize/base",
      "projectFolder": "packages/base"
    },
    {
      "packageName": "@organize/layout",
      "projectFolder": "packages/layout"
    },
    {
      "packageName": "@organize/typescript",
      "projectFolder": "common/tools/typescript",
      "shouldPublish": false
    }
  ]
}
