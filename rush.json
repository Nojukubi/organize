{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.70.0",
  "pnpmVersion": "6.32.13",
  "projectFolderMaxDepth": 5,
  "suppressNodeLtsWarning": true,
  "ensureConsistentVersions": true,
  "projects": [
    {
      "packageName": "@organize/website",
      "projectFolder": "apps/website"
    },
    {
      "packageName": "@organize/mobile",
      "projectFolder": "apps/mobile"
    },
    {
      "packageName": "@framework/base",
      "projectFolder": "packages/framework/base"
    },
    {
      "packageName": "@framework/layout",
      "projectFolder": "packages/framework/layout"
    },
    {
      "packageName": "@internal/builder",
      "projectFolder": "common/tools/builder",
      "shouldPublish": false
    },
    {
      "packageName": "@internal/typescript",
      "projectFolder": "common/tools/typescript",
      "shouldPublish": false
    }
  ]
}
